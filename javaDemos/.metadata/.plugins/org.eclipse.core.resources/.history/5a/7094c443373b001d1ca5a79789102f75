package mükemmelSayi;

public class Main {

	public static void main(String[] args) {
		int number = 5;
		int total = 0;
		for (int i = 1; i < 7; i++) {
			if (number % i == 0) {
				total = total + i;
				return;
			}
			if (total == number) {
				System.out.println("Sayı Mükemmel Sayı");
			} else {
				System.out.println("Sayı Mükemmel Sayı Değil");
			}
		}
	}

}
