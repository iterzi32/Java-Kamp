package loopDemo;

public class Main {

	public static void main(String[] args) {
		//For
		for (int i = 1; i < 10; i++) {
			System.out.println(i);
		}
		System.out.println("Döngü bitti");
		
		//While
		int i = 0;
		while (i < 10) {
			System.out.println(i);
			i++;
		}
		System.out.println("While döngüsü bitti");
	}

}
