package multiDimensional;

public class Main {

	public static void main(String[] args) {
		String[][] cities = new String[3][3];
		
		cities[0][0] = "İstanbul";
		cities[0][1] = "Bursa";
		cities[0][2] = "Bilecik";
		cities[1][0] = "Ankara";
		cities[1][1] = "Konya";
		cities[1][2] = "Kırşehir";
		cities[2][0] = "Diyarbakır";
		cities[2][1] = "Şanlıurfa	";
		cities[2][2] = "Gaziantep";
	}

}
