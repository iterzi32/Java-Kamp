package recapDemo1;

public class Main {

	public static void main(String[] args) {
		int sayi1 = 20;
		int sayi2 = 25;
		int sayi3 = 2;
		
		if (sayi1 > sayi2) {
			if (sayi1 > sayi3) {
				System.out.println("Sayı1 en büyük");
			}
		}
		if (sayi2 > sayi3) {
			System.out.println("Sayı2 en büyük");
		}
		else
		{
			System.out.println("Sayı3 en büyüktür");
		}
		
	}

}
